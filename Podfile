# Uncomment the next line to define a global platform for your project
 platform :ios, '13.0'
workspace 'ModularTest.xcworkspace'
project 'ModularTest.xcodeproj'

def corePod
     pod 'DummyCore', :path => 'DevPods/DummyCore'
 end


target 'ModularTest' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  pod 'DummyCore', :path => 'DevPods/DummyCore'
 end